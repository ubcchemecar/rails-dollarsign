<div class="row">
  <div class="col-md-6">
    <h3> Items Ordered </h3>
    <p>
    <%= line_chart Record.group_by_week(:created_at) %>
    </p>
  </div>
  <div class="col-md-6">
    <h3> Spending Per Month </h3>
    <p>
    <%= line_chart Record.group_by_month(:created_at, "sum", "price*quantity") %>
    </p>
  </div>  
</div>